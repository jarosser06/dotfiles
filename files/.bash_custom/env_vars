#bin/bash

export CHEFDK_LOCAL_BIN=${HOME.chefdk/gem/ruby/2.1.0/bin
export EDITOR=vim
export GOPATH=${HOMEGo
export CUSTOM_BIN=${HOME.bin
export COOKBOOKS=${HOMEcookbooks
export PATH=${HOME.bin:/usr/local/go/bin:${GOPATH}/bin:$PATH
## Adds dart to the path
export PATHusr/local/dart/bin:$PATH

if [ -a ${HOME.rvm ]; then
  PATH=$PATH:${HOME.rvm/bin
fi
export PROJECTS_DIR=${HOMEProjects
export PROJECTS_LIST=${HOME.projects

## Needed for Rust
export LD_LIBRARY_PATHusr/local/lib

## Used for git completion
PS1='[\u@\h] \w\[\033[33m\]$(__git_ps1)\[\033[00m\]\$ '
GIT_PS1_SHOWDIRTYSTATE=true

if [ -fusr/bin/virtualenvwrapper.sh ];
then
  sourceusr/bin/virtualenvwrapper.sh
else
  echo "Didn't find virtualenvwrapper.sh"
fi

export WORKON_HOME=$HOM.virtualenvs
export PIP_VIRTUALENV_BASE=$WORKON_HOME
export PIP_RESPECT_VIRTUALENV=true
